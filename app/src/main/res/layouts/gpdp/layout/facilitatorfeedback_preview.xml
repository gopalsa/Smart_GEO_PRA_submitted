        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:ellipsize="end"
        android:maxLines="1"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat"
        />
</LinearLayout>
    <LinearLayout
